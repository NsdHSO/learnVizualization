function lsop
    lsof -i :$argv

   echo "lsof -i :$argv"
end

function y3lok
    rm -rf yarn.lock;
    touch yarn.lock;
    yarn install;
end

function y3all
    rm -rf yarn.lock;
    rm -rf node_module;
     touch yarn.lock;
    yarn install;
end

